{
  "name": "@anvil-vault/framework",
  "version": "0.0.3",
  "keywords": [
    "cardano",
    "anvil",
    "vault",
    "crypto",
    "cryptography",
    "blockchain",
    "custodial",
    "secure",
    "wallet",
    "javascript",
    "typescript",
    "ts",
    "typesafe"
  ],
  "author": "Cardano-Forge",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./src/index.ts",
  "scripts": {
    "build": "tsup --config build.tsup.ts",
    "check": "tsc --noEmit",
    "emit": "tsup --config emit.tsup.ts",
    "test": "vitest --run --passWithNoTests",
    "test:watch": "vitest --watch --passWithNoTests",
    "publish": "tsx publish.ts"
  },
  "dependencies": {
    "@anvil-vault/bip39": "*",
    "@anvil-vault/cms": "*",
    "@anvil-vault/csl": "*",
    "@anvil-vault/express": "*",
    "@anvil-vault/handler": "*",
    "@anvil-vault/utils": "*",
    "@anvil-vault/vault": "*"
  },
  "devDependencies": {
    "@anvil-vault/tsconfig": "*",
    "@anvil-vault/tsup": "*",
    "tsup": "^8.2.4",
    "tsx": "^4.20.4",
    "typescript": "^5.6.2",
    "vitest": "^2.1.4"
  }
}
